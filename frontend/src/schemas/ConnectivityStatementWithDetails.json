{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "definitions": {},
  "description": "Connectivity Statement with details",
  "properties": {
    "ans_division": {
      "type": "number"
    },
    "apinatomy_model": {
      "type": "string"
    },
    "biological_sex": {
      "type": "string"
    },
    "circuit_type": {
      "enum": [
        "SENSORY",
        "MOTOR",
        "INTRINSIC",
        "PROJECTION",
        "ANAXONIC",
        "UNKNOWN"
      ],
      "type": "string"
    },
    "destination": {
      "type": "number"
    },
    "destination_type": {
      "enum": [
        "AXON-SE",
        "AXON-T",
        "AXON-ST",
        "UNKNOW"
      ],
      "type": "string"
    },
    "has_notes": {
      "type": "boolean"
    },
    "id": {
      "type": "number"
    },
    "knowledge_statement": {
      "type": "string"
    },
    "laterality": {
      "enum": [
        "IPSI",
        "ONTRAT",
        "BI",
        "UNKNOWN"
      ],
      "type": "string"
    },
    "modified_date": {
      "type": "string"
    },
    "origin": {
      "type": "number"
    },
    "owner": {
      "additionalProperties": false,
      "description": "User",
      "properties": {
        "date_joined": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "first_name": {
          "type": "string"
        },
        "groups": {
          "description": "The groups this user belongs to. A user will get all permissions granted to each of their groups.",
          "items": {
            "type": "number"
          },
          "type": "array"
        },
        "id": {
          "type": "number"
        },
        "is_active": {
          "description": "Designates whether this user should be treated as active. Unselect this instead of deleting accounts.",
          "type": "boolean"
        },
        "is_staff": {
          "description": "Designates whether the user can log into this admin site.",
          "type": "boolean"
        },
        "is_superuser": {
          "description": "Designates that this user has all permissions without explicitly assigning them.",
          "type": "boolean"
        },
        "last_login": {
          "type": "string"
        },
        "last_name": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "user_permissions": {
          "description": "Specific permissions for this user.",
          "items": {
            "type": "number"
          },
          "type": "array"
        },
        "username": {
          "description": "Required. 150 characters or fewer. Letters, digits and @/./+/-/_ only.",
          "type": "string"
        }
      },
      "required": [
        "id",
        "password",
        "username"
      ],
      "type": "object"
    },
    "sentence": {
      "type": "number"
    },
    "species": {
      "items": {
        "type": "number"
      },
      "type": "array"
    },
    "state": {
      "type": "string"
    },
    "tags": {
      "items": {
        "additionalProperties": false,
        "description": "Note Tag",
        "properties": {
          "id": {
            "type": "number"
          },
          "tag": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "tag"
        ],
        "type": "object"
      },
      "type": "array"
    }
  },
  "required": [
    "id",
    "has_notes",
    "owner",
    "tags",
    "knowledge_statement",
    "state",
    "modified_date",
    "sentence"
  ],
  "type": "object"
}